{
  "name": "PastaPaste",
  "description": "A plug-in to keep track of text you want to copy and paste!",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_title:": "Open the popup"
  },
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "PASTAPASTE128.png",
    "48": "PASTAPASTE128.png",
    "128": "PASTAPASTE128.png"
  }
}
